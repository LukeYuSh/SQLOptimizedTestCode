<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.dao.GiftTakeDao">

    <resultMap id="dataMap" type="com.test.entity.GiftTake">
        <id column="id" property="id"/>
        <result column="user_id" property="userId"/>
        <result column="gift" property="gift"/>
        <result column="expire_time" property="expireTime"/>
    </resultMap>

    <insert id="batchInsertGiftTake" parameterType="com.test.entity.GiftTake">
        INSERT INTO gift_take(`user_id`, `gift`, `expire_time`)VALUES
        <foreach collection="list" item="item" separator=",">
            (#{item.userId},#{item.gift},#{item.expireTime})
        </foreach>
    </insert>

</mapper>